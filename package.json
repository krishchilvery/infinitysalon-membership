{
  "name": "membership",
  "version": "0.1.0",
  "private": true,
  "author": "Krish Chilvery",
  "description": "A small membership client based on firebase",
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "serve-handler": "^6.1.3"
  },
  "main": "client.js",
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "start-prod": "cross-env BROWSER=none npm start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"cross-env BROWSER=none npm start\" \"wait-on tcp:3000 && electron .\"",
    "electron": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make --arch=ia32,x64"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@material-ui/pickers": "^3.3.10",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "concurrently": "^6.1.0",
    "cross-env": "^7.0.3",
    "electron": "^12.0.7",
    "electron-squirrel-startup": "^1.0.0",
    "firebase": "^8.5.0",
    "firebaseui": "^4.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-firebaseui": "^5.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "wait-on": "^5.3.0",
    "web-vitals": "^1.1.2"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "a",
        "ignore": "membership/src/*",
        "asar": true
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "membership"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
